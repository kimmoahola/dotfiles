{
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "editor.scrollBeyondLastLine": false,
  "editor.tabSize": 2,
  "editor.minimap.enabled": true,
  "editor.rulers": [120],
  "editor.minimap.showSlider": "always",
  "workbench.editor.revealIfOpen": true,
  "tslint.exclude": "node_modules",
  "workbench.editor.labelFormat": "short",
  "editor.minimap.side": "left",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "breadcrumbs.enabled": true,
  "editor.formatOnSave": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "window.zoomLevel": 0,
  "tslint.autoFixOnSave": true,
  "gitlens.views.repositories.files.layout": "auto",
  "gitlens.codeLens.enabled": false,
  "python.jediEnabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "vscode-icons",
  "codemetrics.nodeconfiguration.ReturnStatement": 0.1,
  "codemetrics.nodeconfiguration.CaseClause": 0.1,
  "codemetrics.nodeconfiguration.DefaultClause": 0.1,
  "editor.formatOnPaste": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": true,
  "codemetrics.basics.CodeLensHiddenUnder": 10,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "codemetrics.nodeconfiguration.VariableStatement": 0.1,
  "codemetrics.nodeconfiguration.VariableDeclaration": 0.1,
  "codemetrics.nodeconfiguration.TypeReference": 0.1,
  "codemetrics.nodeconfiguration.TypeLiteral": 0.1,
  "codemetrics.nodeconfiguration.TypeAssertionExpression": 0.1,
  "codemetrics.nodeconfiguration.TemplateExpression": 0.1,
  "codemetrics.nodeconfiguration.SourceFile": 0.1,
  "codemetrics.nodeconfiguration.SetAccessor": 0.1,
  "codemetrics.nodeconfiguration.RegularExpressionLiteral": 0.1,
  "codemetrics.nodeconfiguration.PropertySignature": 0.1,
  "codemetrics.nodeconfiguration.PropertyDeclaration": 0.1,
  "codemetrics.nodeconfiguration.PropertyAssignment": 0.1,
  "codemetrics.nodeconfiguration.PropertyAccessExpression": 0.1,
  "codemetrics.nodeconfiguration.PrefixUnaryExpression": 0.1,
  "codemetrics.nodeconfiguration.PostfixUnaryExpression": 0.1,
  "codemetrics.nodeconfiguration.Parameter": 0.1,
  "codemetrics.nodeconfiguration.ObjectLiteralExpression": 0.1,
  "codemetrics.nodeconfiguration.ObjectBindingPattern": 0.1,
  "codemetrics.nodeconfiguration.NewExpression": 0.1,
  "codemetrics.nodeconfiguration.NamespaceImport": 0.1,
  "codemetrics.nodeconfiguration.NamedImports": 0.1,
  "codemetrics.nodeconfiguration.ModuleDeclaration": 0.1,
  "codemetrics.nodeconfiguration.MethodSignature": 0.1,
  "codemetrics.nodeconfiguration.InterfaceDeclaration": 0.1,
  "codemetrics.nodeconfiguration.IndexSignature": 0.1,
  "codemetrics.nodeconfiguration.ImportEqualsDeclaration": 0.1,
  "codemetrics.nodeconfiguration.ImportDeclaration": 0.1,
  "codemetrics.nodeconfiguration.GetAccessor": 0.1,
  "codemetrics.nodeconfiguration.ExpressionStatement": 0.1,
  "codemetrics.nodeconfiguration.ElementAccessExpression": 0.1,
  "codemetrics.nodeconfiguration.DebuggerStatement": 0.1,
  "codemetrics.nodeconfiguration.ConstructorType": 0.1,
  "codemetrics.nodeconfiguration.ClassDeclaration": 0.1,
  "codemetrics.nodeconfiguration.CallSignature": 0.1,
  "codemetrics.nodeconfiguration.CallExpression": 0.1,
  "codemetrics.nodeconfiguration.Block": 0.1,
  "codemetrics.nodeconfiguration.ArrayLiteralExpression": 0.1,
  "codemetrics.nodeconfiguration.ArrayBindingPattern": 0.1,
  "codemetrics.nodeconfiguration.JsxElement": 0.2,
  "codemetrics.nodeconfiguration.JsxSelfClosingElement": 0.1,
  "editor.fontFamily": "'Fira Code', monospace",
  "editor.fontLigatures": true
}
